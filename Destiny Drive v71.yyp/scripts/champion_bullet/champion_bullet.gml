function champion_bullet(argument0) {
	if champ = c_orange
	{
	    with(argument0)
	    {
	    damage += 1
	    }
	}

	if champ = c_yellow
	{
	    with(argument0)
	    {
	    spd *= 1.2
	    }
	}

	if champ = c_blue
	{
	    with(argument0)
	    {
	    shield_breaker = true
	    }
	}

	if champ = c_ltgray
	{
	    with(argument0)
	    {
	    wrap = true
	    }
	}



}
